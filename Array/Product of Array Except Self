Approach-1(Brute Force Approach)

class Solution {
public:
    
    vector<int> productExceptSelf(vector<int>& nums) {
        vector<int>res;
        int n=nums.size();
        
        for(int i=0;i<n;i++)
        {
            res.push_back(1);
            for(int j=0;j<n;j++)
            {
                if(i!=j)
                    res[i]*=nums[j];
            }
        }
        return res;
    }
};
