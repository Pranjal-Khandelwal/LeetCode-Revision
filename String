Brute Force
T.C-O(N^2)
S.C-O(1)

class Solution {
public:
    int strStr(string haystack, string needle) {
        int m=haystack.size();
        int n=needle.size();
    
        int i=0,j=0;
        for(i=0;i<=m-n;i++)
        {   
            for(j=0;j<n;j++)
            {
                if(haystack[i+j]!=needle[j]){
                    break;
            }
        }
        
        if(j==n){
            return i;
        }
    }
    
    return -1;
        
        
    }
};
