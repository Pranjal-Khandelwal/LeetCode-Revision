T.C-O(N^3 LOGN)
S.C-O(1)

class Solution {
public:
    int nthUglyNumber(int n) {
        vector<int>v;
        
        if(v.empty()){
            for(long i=1;i<=INT_MAX;i*=2)
                for(long j=i;j<=INT_MAX;j*=3)
                    for(long k=j;k<=INT_MAX;k*=5)
        
                        v.push_back(k);
        sort(v.begin(),v.end());
        }
        
        return v[n-1];
    }
};
