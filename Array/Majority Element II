Approach-(Using Unordered_map)
T.C.-O(N)
S.C.-O(N)

class Solution {
public:
    vector<int> majorityElement(vector<int>& nums) {
        int n=nums.size();
        vector<int>res;
        unordered_map<int,int>mp;
        
        for(int i=0;i<n;i++)
        {
            mp[nums[i]]++;
        }
        
        for(auto x:mp){
          if(x.second>n/3)
                res.push_back(x.first);  
        }
        return res;
    }
};
