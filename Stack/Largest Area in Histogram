Approach-1(Brute Force)
T.C-O(N^2)
S.C-O(1)

class Solution {
public:
    int largestRectangleArea(vector<int>& heights) {
        int maxArea=0;
        int n=heights.size();
        for(int i=0;i<n;i++)
        {
         int height=heights[i];
            
            for(int j=i;j<n;j++)
            {
                height=min(height,heights[j]);
                maxArea=max(maxArea,height*(j-i+1));
            }
                
        }
        return maxArea;
    }
};
