Approach-1(Brute Force)
T.C-O(N^3)
S.C-O(1)

class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        int n=nums.size();
        int count=0;
        
        for(int i=0;i<n;i++)
        {
            for(int j=i+1;j<=n;j++)
            {
                 int sum=0;
                for(int k=i;k<j;k++)
                    sum+=nums[k];
                
                if(sum==k)
                    count++;
            }
        }
        return count;
    }
};
