class Solution {
public:
    int minSteps(string s, string t) {
        unordered_map<char,int>mp;
        
        int sum=0;
        
        for(auto x:s)
        {
            mp[x]++;
        }
        
        for(auto x:t)
        {
            mp[x]--;
        }
        
        for(auto x:mp)
        {
            if(x.second<0)
                sum+=x.second;
        }
        
        return abs(sum);
        
    }
};
