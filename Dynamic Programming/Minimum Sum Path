Approach-1(Recursion)
class Solution {
public:
    int minsum(vector<vector<int>>&grid,int m,int n)
    {
        if(m<0 || n<0)
            return INT_MAX;
        if(m==0 && n==0)
            return grid[m][n];
        
        return grid[m][n]+min(minsum(grid,m-1,n),minsum(grid,m,n-1));
    }
    int minPathSum(vector<vector<int>>& grid) {
      int m=grid.size();
      int n=grid[0].size();
        return minsum(grid,m-1,n-1);
        
        
        
    }
}; 
